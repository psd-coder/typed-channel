{
  "name": "typed-channel",
  "version": "0.9.4",
  "description": "A type-safe communication channel for sending and receiving messages between different contexts in a TypeScript environment",
  "keywords": [
    "typed",
    "types",
    "typescript",
    "channel",
    "transport",
    "communication",
    "events",
    "worker",
    "postmessage"
  ],
  "repository": "psd-coder/typed-channel",
  "author": "Pavel Grinchenko <psdcoder@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/*",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "sideEffects": false,
  "module": "./dist/index.js",
  "scripts": {
    "build": "tsup",
    "examples": "vite ./examples --open",
    "lint": "pnpm --parallel /^lint:*/",
    "lint:oxlint": "oxlint",
    "lint:prettier": "prettier --check .",
    "format": "prettier . --write",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "bump": "./scripts/bump.sh"
  },
  "packageManager": "pnpm@10.8.1",
  "devDependencies": {
    "oxlint": "^0.16.6",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.1",
    "vitest": "^3.1.1"
  }
}
